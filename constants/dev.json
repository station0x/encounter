  "spaceshipsAttributes": {
    "base": {
      "hp": 100,
      "desc": "Base is the bumping heart of the spaceships fleet. It's an immobile spacecraft that control and command all of your spaceships. Your mission is to defend yours and destroy the enemy's."
    },
    "battlestar": {
      "attack": 25,
      "hp": 65,
      "moveFuelCost": 2,
      "attackFuelCost": 3,
      "desc": "Battlestar is tough and have moderately high damage. It's balanced. It have 25 attack damage, 65 hp and consumes 2 fuel per turn."
    },
    "thunderbird": {
      "attack": 15,
      "hp": 35,
      "moveFuelCost": 1,
      "attackFuelCost": 1,
      "desc": "Thunderbirds are the most mobile spaceships but also squishy. It consumes only 1 fuel per move, 1 fuel only to attack and has an attack damage of 20. It's undoubtly the most unpredictable spaceship that can surprise your enemy."
    },
    "salvation": {
      "repairPercent": 25,
      "hp": 50,
      "moveFuelCost": 2,
      "repairFuelCost": 2,
      "desc": "Salvation is a healer. It cannot attack other spaceships but can repair your own. If you lose it, you cannot repair your spaceships anymore! You can only heal one spaceship per turn, use it wisely."
    },
    "gunship": {
      "attack": 30,
      "hp": 60,
      "moveFuelCost": 2,
      "attackFuelCost": 3,
      "desc": "Gunships have the highest damage but is fairly damagable. Don't solo it unless you know what are you doing."
    },
    "colossus": {
      "attack": 20,
      "hp": 150,
      "moveFuelCost": 3,
      "attackFuelCost": 3,
      "desc": "Colossus is the tankiest spaceship with the highest HP in the game. It's not very mobile and has a relatively low attack damage."
    },
    "ubiquity": {
      "attack": 25,
      "hp": 50,
      "moveFuelCost": 2,
      "attackFuelCost": 3,
      "warp": true,
      "desc": "Ubiquity spaceships can warp from one edge to another when placed adjacent to the left or right edges of the board."
    },
    "nightstalker": {
      "attack": 15,
      "hp": 55,
      "moveFuelCost": 2,
      "attackFuelCost": 3,
      "bonusAttackOnAttack": 8,
      "desc": "Nightstalker gain 8 attack damage if it lands an attack to any other spaceship with unlimited stack amount. It's squishy with a base attack of 15."
    },
    "orkocraft": {
      "attack": 10,
      "hp": 55,
      "moveFuelCost": 2,
      "attackFuelCost": 3,
      "shock": true,
      "desc": "Orkocraft creates an electromagnetic shockwave that propogates through all adjacent units causing 10 damage to each unit. (including your fleet, Except your base). It's very effecient against defensive structures."
    },
    "vampship": {
      "attack": 15,
      "hp": 50,
      "vamp": 15,
      "moveFuelCost": 0,
      "attackFuelCost": 0,
      "moveHpCost": 7,
      "desc": "Vampship is a fuel-less spaceship that doesn't need FUEL to move or attack. Moving only costs HP, that can be refunded by attacking other units (stealing 15 of their HP per attack)."
    },
    "divinewind": {
      "attack": 15,
      "hp": 60,
      "onDestroyDamage": 15,
      "moveFuelCost": 2,
      "attackFuelCost": 3,
      "desc": "DivineWind spaceship have balanced stats between attack, defense and mobility. When it's destroyed it explodes dealing 15 extra damage to the spaceships adjacent to it."
    }
  }
